{% extends 'base.html.twig' %}

{% block title %}Inventaires{% endblock %}

{% block body %}
    <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
        <!--begin::Toolbar-->
        <div class="toolbar" id="kt_toolbar">
            <!--begin::Container-->
            <div id="kt_toolbar_container" class="container-fluid d-flex flex-stack">
                <!--begin::Page title-->
                <div data-kt-swapper="true" data-kt-swapper-mode="prepend" data-kt-swapper-parent="{default: '#kt_content_container', 'lg': '#kt_toolbar_container'}" class="page-title d-flex align-items-center flex-wrap me-3 mb-5 mb-lg-0">
                    <!--begin::Title-->
                    <h1 class="d-flex text-dark fw-bolder fs-3 align-items-center my-1">Inventaire</h1>
                    <!--end::Title-->
                    <!--begin::Separator-->
                    <span class="h-20px border-gray-300 border-start mx-4"></span>
                    <!--end::Separator-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-separatorless fw-bold fs-7 my-1">
                        <!--begin::Item-->
                        <li class="breadcrumb-item text-muted">
                            <a href="{{ path('app_admin_dashboard') }}" class="text-muted text-hover-primary">Accueil</a>
                        </li>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <li class="breadcrumb-item">
                            <span class="bullet bg-gray-300 w-5px h-2px"></span>
                        </li>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <li class="breadcrumb-item text-muted">Gestion de stock</li>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <li class="breadcrumb-item">
                            <span class="bullet bg-gray-300 w-5px h-2px"></span>
                        </li>
                        <!--end::Item-->
                        <!--begin::Item-->
                        <li class="breadcrumb-item text-dark">Inventaire</li>
                        <!--end::Item-->
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page title-->

                 <!--begin::Menu-->
                    <div class=" menu menu-lg-rounded menu-column menu-lg-row menu-state-bg menu-title-gray-700 menu-state-title-primary menu-state-icon-primary menu-state-bullet-primary menu-arrow-gray-400 fw-bold my-5 my-lg-0 align-items-stretch"
                    style="width : auto !important;"
                        id="#kt_header_menu" data-kt-menu="true">
                        <div data-kt-menu-trigger="click" data-kt-menu-placement="bottom-start"
                             class="menu-item here show menu-lg-down-accordion me-lg-1">
                            <span class="menu-link py-3">
                                <span class="menu-title">Sélectionner  une option dans la liste </span>
                                <span class="menu-arrow d-lg-none"></span>
                            </span>
                            <div
                                class="menu-sub menu-sub-lg-down-accordion menu-sub-lg-dropdown menu-rounded-0 py-lg-4 w-lg-225px">
                                <div class="menu-item">
                                    <a class="menu-link py-3 kt-menu-proto active" href="javascript:void(0)" id="kt_inventaire_form" onclick="activateMenu(this);"
                                        data-content="{% autoescape 'html' %}{{ include('inventaire/partials/_card_form.html.twig', {'idMenu':'kt_inventaire_list'})|escape }}{% endautoescape %}">
                                        <span class="menu-bullet">
                                            <span class="bullet bullet-dot"></span>
                                        </span>
                                        <span class="menu-title">Ajouter un inventaire</span>
                                    </a>
                                </div>
                                <div class="menu-item">
                                    <a class="menu-link py-3 kt-menu-proto" href="javascript:void(0)" id="kt_inventaire_list" onclick="activateMenu(this);"
                                        data-content="{% autoescape 'html' %}{{ include('inventaire/partials/_card_list.html.twig', {'idMenu':'kt_inventaire_list'})|escape }}{% endautoescape %}">
                                        <span class="menu-bullet">
                                            <span class="bullet bullet-dot"></span>
                                        </span>
                                        <span class="menu-title">Liste des inventaires</span>
                                    </a>
                                </div>
                                <div class="menu-item">
                                    <a class="menu-link py-3 kt-menu-proto" type="button" onclick="imprime()">
                                     {# href="javascript:void(0)" id="kt_inventaire_fiche" onclick="activateMenu(this);"
                                        data-content="{% autoescape 'html' %}{{ include('inventaire/partials/_fiche_inv.html.twig', {'idMenu':'kt_inventaire_list'})|escape }}{% endautoescape %}"> #}
                                        <span class="menu-bullet">
                                            <span class="bullet bullet-dot"></span>
                                        </span>
                                        <span class="menu-title">Fiche d'inventaire</span>
                                    </a>
                                </div>
                                <div class="menu-item">
                                    <a class="menu-link py-3 kt-menu-proto d-none remove" href="javascript:void(0)" id="kt_inv_show" onclick="activateMenu(this);">
                                        <span class="menu-bullet">
                                            <span class="bullet bullet-dot"></span>
                                        </span>
                                        <span class="menu-title">Détails</span>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                <!--end::Menu-->

            </div>
            <!--end::Container-->
        </div>
        <!--end::Toolbar-->
        <div class="post d-flex flex-column-fluid" id="kt_post">
            <div id="kt_content_container" class="container-xxl">
                <!--begin::Inbox App - Messages -->
                <div class="d-flex flex-column flex-lg-row">
                   
                    <!--begin::Content-->
                    <div class="flex-lg-row-fluid ms-lg-7 ms-xl-10">
                        <div class="col flex-row-fluid" data-call-form="ajax"></div>
                    </div>
                    <!--end::Content-->
                </div>
                <!--end::Inbox App - Messages -->
            </div>
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}