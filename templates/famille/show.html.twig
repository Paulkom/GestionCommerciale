{% extends 'base.html.twig' %}

{% block title %}Famille{% endblock %}

{% block body %}
    <h1>Famille</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ famille.id }}</td>
            </tr>
            <tr>
                <th>CodeFamille</th>
                <td>{{ famille.codeFamille }}</td>
            </tr>
            <tr>
                <th>LibelleFamille</th>
                <td>{{ famille.libelleFamille }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ famille.createdAt ? famille.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ famille.updatedAt ? famille.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_famille_index') }}">back to list</a>

    <a href="{{ path('app_famille_edit', {'id': famille.id}) }}">edit</a>

    {{ include('famille/_delete_form.html.twig') }}
{% endblock %}
