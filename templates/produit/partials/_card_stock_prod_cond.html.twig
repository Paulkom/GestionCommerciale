 <!--begin::Contacts-->
<div class="card card-flush" data-menu="{% if idMenu is defined %}{{ idMenu }}{% endif %}">
    <!--begin::Card body-->
    <div class="card-body pt-5">
        <div class="table-responsive">
            <!--begin::Table-->
            <table class="table align-middle table-row-dashed fs-6 gy-5"
                   data-request="ajaxify-search" data-entity="App\Entity\ProduitConditionnement" data-perpage="10">
                <!--begin::Table head-->
                <thead>
                <!--begin::Table row-->
                <tr class="text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0">
                    <th class="w-10px pe-2">
                        <div class="form-check form-check-sm form-check-custom form-check-solid me-3">
                            <input class="form-check-input" type="checkbox" data-kt-check="true"
                                   data-kt-check-target="#kt_produits_table .form-check-input"
                                   value="1"/>
                        </div>
                    </th>
                    <th class="w-300px">Famille</th>
                    <th class="min-w-500px">Produits & Conditionnements</th>
                    <th class="w-100px">Qté Cond.</th>
                    <th class="w-100px">Qté Stock</th>
                </tr>
                <!--end::Table row-->
                </thead>
                <!--end::Table head-->
                <!--begin::Table body-->
                <tbody class="fw-bold text-gray-600">
                </tbody>
                <!--end::Table body-->
            </table>
            <!--end::Table-->
        </div>
    </div>
    <!--end::Card body-->
</div>
<!--end::Produits-->